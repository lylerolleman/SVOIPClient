<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>


<GridPane fx:id="gp" prefHeight="78.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.LaunchController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <children>
      <HBox prefHeight="0.0" prefWidth="200.0">
         <children>
            <TextField fx:id="idbox" prefHeight="31.0" prefWidth="151.0" promptText="Enter Nickname" />
            <Button fx:id="launchbutton" mnemonicParsing="false" onAction="#launch" text="Go" />
         </children>
      </HBox>
   </children>
</GridPane>
